# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Your Supabase project URL (found in your Supabase project settings)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key (found in your Supabase project settings)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Backend API URL (for production, use your deployed backend URL)
VITE_API_URL=http://localhost:8080

# GitHub Pages deployment flag (set to 'true' when deploying to GitHub Pages)
# VITE_GITHUB_PAGES=true
